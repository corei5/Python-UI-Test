
config system
	option zonename 'UTC'
	option timezone 'GMT0'
	option conloglevel '8'
	option cronloglevel '8'
	option hostname 'OpenWrt_shamim'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '1'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'
	list server '0.0.0.0'
	list server '0.0.0.0'
	list server '1.1.1.1'
	list server '2.2.2.2'

config system
	option zonename 'America/Argentina/Catamarca'
	option timezone 'GMT0'
	option date '04/22/2016'
	option time '08/15/53'
	option daylightsaving '1'
	option start_month '0'
	option start_count '1'
	option start_week '3'
	option start_hour '3'
	option end_month '1'
	option end_count '2'
	option end_week '4'
	option end_hour '4'

