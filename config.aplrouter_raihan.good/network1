
config interface 'test'
	option ifname 'eth0.2'
	option _orig_ifname 'eth0.2'
	option _orig_bridge 'false'
	option proto 'static'
	option ipaddr '192.168.11.150'
	option netmask '255.255.255.0'
	option gateway '192.168.11.1'
	option dns '8.8.8.8'

config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd61:2bde:c2d9::/48'

config interface 'lan'
	option _orig_ifname 'eth0.1 radio0.network1 wlan0'
	option _orig_bridge 'true'
	option ifname 'eth0.1'
	option type 'bridge'
	option proto 'pppoe'
	option username 'tedy'
	option password '123'
	option c_password '321'
	option mtu '1500'
	list dns ''
	option ipaddr '192.168.0.2'
	option enabled '255.0.0.0'
	option netmask '255.0.0.0'

config interface 'wan'
	option ifname 'eth0.2'
	option _orig_ifname 'eth0.2'
	option _orig_bridge 'false'
	option wName 'dree'
	option mode '11bg mixed'
	option channelWidth '11bg Auto'
	option radio '1'
	option ssd_proadcast '1'
	option wds_bridge '1'
	option channel '3'
	option proto 'dhcp'
	option mtu '1500'
	option mobile_ISP 'AT&T'
	option country 'BD'
	option access_mode 'wan_preferred'
	option name 'wan'
	option section 'interface'
	option mac '64:09:80:74:80:97'
	option filtering_rule 'deny'
	option mac_filtering_status '1'

config interface 'wan6'
	option ifname 'eth0.2'
	option _orig_ifname 'eth0.2'
	option _orig_bridge 'false'
	option proto 'dhcp'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '1'
	option ports '0 1 2 3 6t'

config switch_vlan
	option device 'switch0'
	option vlan '2'
	option ports '4 6t'

config interface 'wan2'
	option proto '3g'
	option metric '20'
	option service 'evdo'
	option username 'waps'
	option password 'waps'
	option device '/dev/ttyUSB0'
	option dialnumber '#777'

config interface 'wan3'
	option proto 'dhcp'
	option ifname 'eth1'
	option metric '21'

config interface '__wan'
	option _orig_ifname 'eth0.2'
	option ifname 'eth0.2'
	option section 'interface'
	option netmask '255.255.255.0'
	option proto 'static'
	option _orig_bridge 'false'
	option ipaddr '255.255.420.420'

config route 'Destimation_Network1'
	option mtu '1500'
	option netmask 'Subnet mAsk'
	option metric '0'
	option name 'Destimation Network1'
	option section 'route'
	option target 'Destimation Network1'
	option status 'enabled'
	option gateway 'default gateway'
	option interface 'radio1'

config interface '___name'
	option _orig_ifname 'eth0.2'
	option ifname 'eth0.2'
	option ipaddr '0.0.0.0'
	option section 'interface'
	option netmask '255.255.255.0'
	option proto 'static'
	option _orig_bridge 'false'

