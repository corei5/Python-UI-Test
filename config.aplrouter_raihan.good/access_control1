
config access_control 'general'
	option enabled '1'

config schedule
	option name 'schedule2'
	option days_option 'selected'
	list days 'wed'
	list days 'thu'
	option time 'on'

config host
	option mode 'ip'
	option name 'Host Entry'
	option ip_start '101'
	option ip_end '202'

config target 'Target_Description1'
	option proto 'tcp'
	option name 'Target Description1'
	option port_end 'Port40'
	option section 'target'
	option ipaddr_end 'Ip Address20'
	option mode 'ip'
	option port_start 'Port 30'
	option ipaddr_start 'IP Address10'

config access_control 'email'
	option auth_req '1'
	option outgoing_server '192.168.1.1'
	option email_address 'raihanhbh@hhh.com'
	option time_mode 'a'
	option notification_status '1'
	option weekday 'monday'
	option name 'email'
	option section 'access_control'
	option immediate_alert '1'
	option mode 'hourly'
	option password 'password'
	option username 'username'
	option time '3:00'

config target 'Target_me'
	option port_end '22'
	option proto 'all'
	option name 'Target me'
	option section 'target'
	option ipaddr_end '102'
	option mode 'ip'
	option port_start '11'
	option ipaddr_start '201'

config rule '707'
	option host 'Host Entry'
	option name '707'
	option section 'rule'
	option target 'Target Description1'
	option status 'enable'
	option schedule 'schedule2'

config host 'raihan_habib'
	option mac 'Mac2'
	option mode 'mac'
	option name 'raihan.habib'
	option section 'host'

config target 'Target_Description10'
	option port_end '21'
	option proto 'tcp'
	option name 'Target Description10'
	option section 'target'
	option ipaddr_end 'Ip Address20'
	option mode 'ip'
	option port_start 'Port 30'
	option ipaddr_start 'IP Address10'

config schedule 'schedule20'
	option days_option 'all'
	option name 'schedule20'
	option section 'schedule'

config rule 'jaha_chai'
	option host 'raihan.habib'
	option name 'jaha chai'
	option section 'rule'
	option target 'Target Description10'
	option status 'disable'
	option schedule 'schedule20'

